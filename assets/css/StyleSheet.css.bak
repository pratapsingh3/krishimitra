.header {
	height: 360px;
}

.cat {
	height: 250px;
	background-repeat: no-repeat;
    background-size: cover;
	border-radius: 36px;
	margin-bottom: 50px;
	opacity: 0.7;
}

.catbg1{
	background-image: url('../img/crop.jpg');
}

.catbg2{
	background-image: url('../img/irrigation.jpg');
}

.catbg3{
	background-image: url('../img/pesticides.jpg');
}

.catbg4{
	background-image: url('../img/govtschemes.jpg');
}



.cat1 {
	margin-left: 150px;
    margin-right: 25px;
}

.cat2 {
	margin-right: 150px;
    margin-left: 25px;
}

.cat h1{
	text-align: center;
	padding-top: 90px;
	color: #fff;
}

.wrapper {
  height: 100%;
  font-family: 'Quicksand', sans-serif;
  line-height: 1.5;
  word-spacing: 4px;
  letter-spacing: 1px;
}

.fixed-bg {
  background-attachment: fixed;
  background-repeat: no-repeat;
  background-size: cover;
  background-position: center center;
  height: 100%; 
  width: 100%;
  color: #fff;
  text-align: center;
  display: table;
}

.fixed-bg h1 {
  vertical-align: middle;
}

.bg-1 {
  background-image: url('../img/aboutban.jpg');
  opacity: 0.8;
}

.contban {
	background-image: url('../img/conban.jpg');
	background-repeat: no-repeat;
    background-size: cover;
}

.t4Left {
	float: left;
}

.t4Right {
	float: right;
}

.t4Clear {
	clear: both;
}

.footline li{
	display: inline;
	padding: 10px;
}

.subscribe {
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
	/* background: white; */
	/* width: 945px; */
	/* height: 474px; */
	/* border-radius: 10px; */
	/* box-shadow: 0 4px 20px rgba(61, 159, 255, 0.2) */
}


.subscribe__title {
	font-weight: bold;
	font-size: 26px;
	margin-bottom: 18px;
}

.subscribe__copy {
	max-width: 450px;
	text-align: center;
	margin-bottom: 53px;
	line-height: 1.5;
}

.form {
	margin-bottom: 25px;
}

.form__email {
	padding: 20px 25px;
	border-radius: 5px;
 	border: 1px solid #CAD3DB;
	width: 431px;
	font-size: 18px;
	color: #0F121F;
}

.form__email:focus {
	outline: 1px solid #3D9FFF;
}

.form__button {
	background: #3dffa5;
	padding: 10px;
	border: none;
	width: 244px;
	height: 65px;
	border-radius: 5px;
	font-size: 18px;
	color: white;
	box-shadow: 0 4px 20px #3dffa5;
}

.form__button:hover {
	box-shadow: 0 10px 20px #000;
}

.notice {
	width: 345px;
}

.feedp{
	font-size: 12px;
}

.feedborder{
	border: 1px solid;
	padding: 10px;
	box-shadow: 4px 9px 12px #888888;
	margin-left: 50px;
}

.concover{
	background-color: #fff;
	margin-left: 50px;
	margin-right: 50px;
}
